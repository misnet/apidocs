{
    "id":"console.menu.list",
    "name":"菜单列表",
    "description":"获取菜单列表",
    "namespace":"Kuga\\Api\\Console",
    "method":"System.listMenu",
    "accessLevel": 2,
    "request":[
        {
            "param":"page",
            "required":false,
            "default":1,
            "name":"页码",
            "type":"Integer",
            "description":"页码"
        },
        {
            "param":"limit",
            "required":false,
            "default":10,
            "type":"Integer",
            "description":"每页显示条数",
            "name":"每页显示条数"
        }
    ],
    "response":{
        "data":{
            "type":"Object",
            "hasSampleFile":false,
            "responseItem": {
                "list":{
                    "type":"Array",
                    "responseItem":{

                        "display":{
                            "type":"Integer",
                            "sample":1,
                            "description":"是否显示，1是，0不是"
                        },
                        "name":{
                            "type":"String",
                            "sample":"控制台",
                            "description":"菜单名称"
                        },
                        "id":{
                            "type":"Integer",
                            "sample":1,
                            "description":"菜单ID"
                        },
                        "parentId":{
                            "type":"Integer",
                            "sample":0,
                            "description":"菜单父级ID"
                        },
                        "url":{
                            "type":"String",
                            "sample":"",
                            "description":"菜单网址"
                        },
                        "sortByWeight":{
                            "type":"Integer",
                            "sample":1,
                            "description":"排序权重"
                        },
                        "extName":{
                            "type":"String",
                            "sample":"article",
                            "description":"扩展插件名称"
                        }
                    },
                    "description":"数据列表"
                },
                "total":{
                    "type":"Integer",
                    "description":"记录总数",
                    "sample":10
                },
                "page":{
                    "type":"Integer",
                    "description":"当前页码",
                    "sample":1
                },
                "limit":{
                    "type":"Integer",
                    "description":"每页条数",
                    "sample":10
                }
            }
        }
    }
}